def find_max_min(arr):
    if not arr:
        return None, None
    max_value = arr[0]
    min_value = arr[0] 
    for num in arr:
        if num > max_value:
            max_value = num
        if num < min_value:
            min_value = num            
    return max_value, min_value
numbers = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
maximum, minimum = find_max_min(numbers)
print(f"Maximum: {maximum}, Minimum: {minimum}")
